package javaHomeWork3;

public class Student {
	private int id;
	private String name;
	private int age;
	
	public int getId(int id) {
		return id;
	}
	
	public void setId() {
		this.id =id;
	}
	public String getName(String name) {
		return name;
	}
	public void setFirstName() {
		this.name=name;
	}
	
	public int getAge(int age) {
		return age;
	}
	public void setAge() {
		this.age=age;
	}
}
