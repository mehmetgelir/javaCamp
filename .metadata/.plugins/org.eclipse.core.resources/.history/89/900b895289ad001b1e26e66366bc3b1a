package classesWithAttributes;

public class Product {
	private int id;
	private String name;
	private String description;
	private double price;
	private int stockAmount;

	
	public int getId() {
		return id;
	}
	
	public void setId(int id) {
		id=id;
	}
	public String getName() {
		
		return name;
	}
}
